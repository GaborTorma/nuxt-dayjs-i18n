{
  "name": "@gabortorma/nuxt-dayjs-i18n",
  "version": "4.0.3",
  "description": "Extend dayjs-nuxt plugin with automatic i18n locale switching.",
  "repository": {
    "url": "git+ssh://git@github.com/GaborTorma/nuxt-dayjs-i18n.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
		"./dayjs/*": "./dist/runtime/dayjs/*"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground --host",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build && nuxt prepare playground",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release && npm publish --access=public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "cleanup": "nuxt cleanup && nuxt cleanup playground",
    "reset": "pnpm cleanup && nuxt prepare && nuxt prepare playground && nuxt-module-build build",
    "reinstall": "rm -rf node_modules && rm -rf ./playground/node_modules && pnpm install && pnpm reset"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "@nuxtjs/i18n": "8.1.1",
    "dayjs-nuxt": "2.1.9"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "3.10.3",
    "@nuxt/test-utils": "3.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@types/node": "^20.11.24",
    "@vue/eslint-config-prettier": "^9.0.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "3.10.3",
    "prettier": "^3.2.5",
    "vitest": "^1.3.1",
    "vue": "^3.4.21"
  }
}
